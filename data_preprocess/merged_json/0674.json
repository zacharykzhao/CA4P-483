        "start": 1370,
        "end": 1387,
        "fields": [],
        "sub_annotations": []
    },
    {
        "classId": "e_27",
        "classId_name": "目的",
        "text": "用于账号管理及登录验证",
        "start": 1458,
        "end": 1469,
        "fields": [],
