        "start": 5141,
        "end": 5161,
        "fields": [],
        "sub_annotations": []
    },
    {
        "classId": "e_25",
        "classId_name": "条件",
        "text": "根据法律、法规的要求、强制性的行政执法",
        "start": 5216,
