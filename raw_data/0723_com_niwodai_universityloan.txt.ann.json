    },
    {
        "classId": "e_25",
        "classId_name": "条件",
        "text": "受本隐私政策中所声明目的的约束",
        "start": 4652,
        "end": 4667,
        "fields": [],
        "sub_annotations": []
    },
    {
        "classId": "e_25",
        "classId_name": "条件",
        "text": "若您未按照您与嘉捷金融及嘉捷金融其他用户签订的协议等法律文本的约定履行应尽义务",
        "start": 4871,
